{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "üöÄ Debug Chan App",
      "program": "${workspaceFolder}/node_modules/@nestjs/cli/bin/nest.js",
      "args": [
        "start",
        "chan",
        "--watch"
      ],
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "restart": true,
      "autoAttachChildProcesses": false, // Êîπ‰∏∫ falseÔºåÈÅøÂÖçÂÜ≤Á™Å
      "sourceMaps": true,
      "smartStep": true,
      "skipFiles": [
        "<node_internals>/**",
        "${workspaceFolder}/node_modules/**"
      ],
      "resolveSourceMapLocations": [
        "${workspaceFolder}/**",
        "!**/node_modules/**"
      ],
      "sourceMapPathOverrides": {
        "webpack:///./*": "${workspaceFolder}/apps/chan/*",
        "webpack:///src/*": "${workspaceFolder}/apps/chan/src/*",
        "webpack:///*": "*",
        "webpack:///./~/*": "${workspaceFolder}/node_modules/*"
      },
      "cwd": "${workspaceFolder}",
      "env": {
        "TS_NODE_PROJECT": "${workspaceFolder}/apps/chan/tsconfig.app.json",
        "NODE_ENV": "development"
      },
      "outputCapture": "std"
    },
    {
      "type": "node",
      "request": "launch",
      "name": "üêû Debug Chan with TS-Node",
      "runtimeArgs": [
        "--nolazy",
        "-r",
        "ts-node/register",
        "-r",
        "tsconfig-paths/register"
      ],
      "args": [
        "${workspaceFolder}/apps/chan/src/main.ts"
      ],
      "cwd": "${workspaceFolder}",
      "sourceMaps": true,
      "env": {
        "NODE_ENV": "development",
        "TS_NODE_PROJECT": "${workspaceFolder}/apps/chan/tsconfig.app.json"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "openOnSessionStart",
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}